# Ex 3

def repeatedChar(my_string):
    my_string.replace(" ", "")
    for i in range(len(my_string)):
        for j in range(i+1, len(my_string)-1):
            if my_string[j] == my_string[i]:
                return "Yes"
    return "No"

print(repeatedChar("the cow jumped over the moon"))
print(repeatedChar("algorithms"))


