# Ex 1

def search(myList, number):
    for i in range(len(myList)):
        if myList[i] == number:
            return True
    return False
    


# Ex 2

my_list = [5, 2, 6, 1, -6, 7, 0]


def adds_to(my_list, k):
    for i in range(len(my_list) - 1):
        for j in range(i+1, len(my_list)):
            if (my_list[i] + my_list[j]) == k:
                return "Yes"
    return "No"
    
